    <script>
        function switchTab(tabName) {
            // Hide all tab contents
            const contents = document.querySelectorAll('.tab-content');
            contents.forEach(content => {
                content.classList.remove('active');
            });
            
            // Remove active class from all buttons
            const buttons = document.querySelectorAll('.tab-btn');
            buttons.forEach(btn => {
                btn.classList.remove('active');
            });
            
            // Show selected tab content
            document.getElementById(tabName).classList.add('active');
            
            // Add active class to clicked button
            event.target.classList.add('active');
        }
        
        function addTrackingRow() {
            const tbody = document.getElementById('trackingBody');
            const newRow = document.createElement('tr');
            newRow.innerHTML = `
                <td><input type="text" placeholder="Name"></td>
                <td><input type="text" placeholder="Organization"></td>
                <td><input type="text" placeholder="Title/Role"></td>
                <td><input type="text" placeholder="Email/Phone"></td>
                <td>
                    <select>
                        <option value="">Select...</option>
                        <option value="new">New Lead</option>
                        <option value="contacted">Contacted</option>
                        <option value="qualified">Qualified</option>
                        <option value="demo">Demo Scheduled</option>
                        <option value="proposal">Proposal Sent</option>
                        <option value="closed">Closed Won</option>
                        <option value="lost">Closed Lost</option>
                    </select>
                </td>
                <td>
                    <select>
                        <option value="">Select...</option>
                        <option value="hot">Hot üî•</option>
                        <option value="warm">Warm ‚òÄÔ∏è</option>
                        <option value="cold">Cold ‚ùÑÔ∏è</option>
                        <option value="pilot">Pilot Interest</option>
                        <option value="explore">Explore</option>
                        <option value="intel">Intel Only</option>
                    </select>
                </td>
                <td><input type="text" placeholder="Next action..."></td>
                <td><input type="date"></td>
                <td><textarea placeholder="Meeting notes, insights..."></textarea></td>
                <td><button class="delete-btn" onclick="deleteRow(this)">Delete</button></td>
            `;
            tbody.appendChild(newRow);
        }
        
        function deleteRow(btn) {
            if (confirm('Are you sure you want to delete this contact?')) {
                const row = btn.parentElement.parentElement;
                row.remove();
            }
        }
        
        function exportTracking() {
            const table = document.getElementById('trackingTable');
            const rows = table.querySelectorAll('tr');
            let csv = [];
            
            // Get headers
            const headers = [];
            rows[0].querySelectorAll('th').forEach(th => {
                headers.push(th.textContent.trim());
            });
            csv.push(headers.join(','));
            
            // Get data rows
            const dataRows = table.querySelectorAll('tbody tr');
            dataRows.forEach(row => {
                const rowData = [];
                const inputs = row.querySelectorAll('input, select, textarea');
                inputs.forEach((input, index) => {
                    if (index < 9) { // Exclude the delete button column
                        let value = input.value || '';
                        // Escape quotes and wrap in quotes if contains comma
                        value = value.replace(/"/g, '""');
                        if (value.includes(',') || value.includes('\n')) {
                            value = '"' + value + '"';
                        }
                        rowData.push(value);
                    }
                });
                csv.push(rowData.join(','));
            });
            
            // Create download
            const csvContent = csv.join('\n');
            const blob = new Blob([csvContent], { type: 'text/csv;charset=utf-8;' });
            const link = document.createElement('a');
            const url = URL.createObjectURL(blob);
            link.setAttribute('href', url);
            link.setAttribute('download', 'schneider_summit_followup_' + new Date().toISOString().split('T')[0] + '.csv');
            link.style.visibility = 'hidden';
            document.body.appendChild(link);
            link.click();
            document.body.removeChild(link);
        }
        
        // Auto-save to localStorage every 30 seconds
        setInterval(() => {
            const tableData = [];
            const rows = document.querySelectorAll('#trackingBody tr');
            rows.forEach(row => {
                const rowData = {};
                const inputs = row.querySelectorAll('input, select, textarea');
                rowData.values = Array.from(inputs).map(input => input.value);
                tableData.push(rowData);
            });
            localStorage.setItem('schneiderTracking', JSON.stringify(tableData));
        }, 30000);
        
        // Load from localStorage on page load
        window.addEventListener('load', () => {
            const saved = localStorage.getItem('schneiderTracking');
            if (saved) {
                const tableData = JSON.parse(saved);
                const tbody = document.getElementById('trackingBody');
                // Only restore if user wants to
                if (confirm('Restore previously saved tracking data?')) {
                    tbody.innerHTML = '';
                    tableData.forEach(rowData => {
                        addTrackingRow();
                        const lastRow = tbody.lastElementChild;
                        const inputs = lastRow.querySelectorAll('input, select, textarea');
                        inputs.forEach((input, index) => {
                            if (rowData.values[index] !== undefined) {
                                input.value = rowData.values[index];
                            }
                        });
                    });
                }
            }
        });
    </script>
</body>
</html>
